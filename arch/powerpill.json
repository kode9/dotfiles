{
    "aria2": {
        "args": [
            "--allow-overwrite=true",
            "--always-resume=false",
            "--auto-file-renaming=false",
            "--auto-file-renaming=false",
            "--check-integrity=false",
            "--conditional-get=true",
            "--connect-timeout=30",
            "--console-log-level=error",
            "--continue=true",
            "--disable-ipv6=true",
            "--download-result=full",
            "--enable-mmap=true",
            "--file-allocation=falloc",
            "--http-no-cache=true",
            "--keep-unfinished-download-result=false",
            "--log-level=warn",
            "--max-concurrent-downloads=64",
            "--max-connection-per-server=4",
            "--max-file-not-found=6",
            "--max-tries=4",
            "--min-split-size=4M",
            "--no-conf=true",
            "--optimize-concurrent-downloads=true",
            "--remote-time=true",
            "--show-console-readout=true",
            "--split=4",
            "--summary-interval=0"
        ],
        "path": "/usr/bin/aria2c"
    },
    "pacman": {
        "config": "/etc/pacman.conf",
        "path": "/usr/bin/pacman"
    },
    "pacserve": {
        "server": null
    },
    "powerpill": {
        "select": true,
        "reflect databases": true
    },
    "reflector": {
        "args": [
            "--protocol",
            "https",
            "--latest",
            "16",
            "--sort",
            "rate",
            "--threads",
            "4",
            "--fastest",
            "8"
        ]
    },
    "rsync": {
        "args": [
            "--checksum",
            "--no-motd",
            "--progress"
        ],
        "db only": true,
        "path": "/usr/bin/rsync",
        "servers": [
            "rsync://archlinux.mailtunnel.eu/archlinux/$repo/os/$arch",
            "rsync://mirror.chaoticum.net/arch/$repo/os/$arch",
            "rsync://mirrors.uni-plovdiv.net/archlinux/$repo/os/$arch",
            "rsync://ger.mirror.pkgbuild.com/packages/$repo/os/$arch",
            "rsync://mirror.neuf.no/archlinux/$repo/os/$arch",
            "rsync://mirrors.atviras.lt/archlinux/$repo/os/$arch",
            "rsync://ftp.nluug.nl/archlinux/$repo/os/$arch",
            "rsync://rsync.mirrors.niyawe.de/archlinux/$repo/os/$arch"
        ]
    }
}
