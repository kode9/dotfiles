{
    "aria2": {
        "args": [
            "--allow-overwrite=true",
            "--always-resume=false",
            "--auto-file-renaming=false",
            "--check-integrity=true",
            "--conditional-get=true",
            "--console-log-level=warn",
            "--continue=true",
            "--file-allocation=none",
            "--http-no-cache=true",
            "--keep-unfinished-download-result=false",
            "--log-level=warn",
            "--max-concurrent-downloads=128",
            "--max-connection-per-server=4",
            "--min-split-size=8M",
            "--no-conf=true",
            "--optimize-concurrent-downloads=true",
            "--remote-time=true",
            "--show-console-readout=true",
            "--summary-interval=0"
        ],
        "path": "/usr/bin/aria2c"
    },
    "pacman": {
        "config": "/etc/pacman.conf",
        "path": "/usr/bin/pacman"
    },
    "pacserve": {
        "server": null
    },
    "powerpill": {
        "select": true,
        "reflect databases": true
    },
    "reflector": {
        "args": [
            "--protocol",
            "https",
            "--latest",
            "16",
            "--sort",
            "rate",
            "--threads",
            "4",
            "--fastest",
            "8"
        ]
    },
    "rsync": {
        "args": [
            "--no-motd",
            "--progress"
        ],
        "db only": true,
        "path": "/usr/bin/rsync",
        "servers": [
            "rsync://archlinux.beccacervello.it/archlinux/$repo/os/$arch",
            "rsync://ger.mirror.pkgbuild.com/packages/$repo/os/$arch",
            "rsync://archlinux.uk.mirror.allworldit.com/archlinux/$repo/os/$arch",
            "rsync://rsync.osbeck.com/archlinux/$repo/os/$arch",
            "rsync://archlinux.polymorf.fr/archlinux/$repo/os/$arch",
            "rsync://mirror.pseudoform.org/packages/$repo/os/$arch",
            "rsync://rsync.myrveln.se/pub/linux/archlinux/$repo/os/$arch",
            "rsync://arch.mirror.constant.com/archlinux/$repo/os/$arch"
        ]
    }
}
