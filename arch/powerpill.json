{
    "aria2": {
        "args": [
            "--allow-overwrite=true",
            "--always-resume=false",
            "--auto-file-renaming=false",
            "--auto-file-renaming=false",
            "--check-integrity=false",
            "--conditional-get=true",
            "--connect-timeout=30",
            "--console-log-level=error",
            "--continue=true",
            "--disable-ipv6=true",
            "--download-result=full",
            "--enable-mmap=true",
            "--file-allocation=falloc",
            "--http-no-cache=true",
            "--keep-unfinished-download-result=false",
            "--log-level=warn",
            "--max-concurrent-downloads=64",
            "--max-connection-per-server=4",
            "--max-file-not-found=6",
            "--max-tries=4",
            "--min-split-size=4M",
            "--no-conf=true",
            "--optimize-concurrent-downloads=true",
            "--remote-time=true",
            "--show-console-readout=true",
            "--split=4",
            "--summary-interval=0"
        ],
        "path": "/usr/bin/aria2c"
    },
    "pacman": {
        "config": "/etc/pacman.conf",
        "path": "/usr/bin/pacman"
    },
    "pacserve": {
        "server": null
    },
    "powerpill": {
        "select": true,
        "reflect databases": true
    },
    "reflector": {
        "args": [
            "--ipv4",
            "--protocol",
            "http,https",
            "--country",
            "AT,BE,DK,FI,FR,DE,IT,LU,NL,NO,ES,SE,GB",
            "--latest",
            "12",
            "--score",
            "8",
            "--fastest",
            "4",
            "--sort",
            "rate",
            "--download-timeout",
            "10"
        ]
    },
    "rsync": {
        "args": [
            "--checksum",
            "--no-motd",
            "--progress"
        ],
        "db only": true,
        "path": "/usr/bin/rsync",
        "servers": [
        ]
    }
}
